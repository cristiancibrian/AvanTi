        <navigation-rule>
              <navigation-case>
                <from-outcome>semaforos</from-outcome>
                <to-view-id>/SemaforosDirector.xhtml</to-view-id>
                <redirect>true</redirect>
            </navigation-case>
        </navigation-rule>
         
