    <!--CASOS DE NAVEGACION DEL COMPONENTE LOGIN Y CATALOGOS DE USUARIOS Y CATALOGOS DE ROLES-->
    <from-view-id>*
       <navigation-rule>
             <navigation-case>
                <from-outcome>consultaract</from-outcome>
                <to-view-id>/RACTConsultas.xhtml</to-view-id>
                <redirect>true</redirect>
            </navigation-case>
        </navigation-rule>
        
