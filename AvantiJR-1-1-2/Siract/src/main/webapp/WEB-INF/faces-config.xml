            <from-view-id>/permisos.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>menuPermiso</from-outcome>
                <to-view-id>/index.xhtml</to-view-id>
                <redirect>true</redirect>
            </navigation-case>
        </navigation-rule>
        
